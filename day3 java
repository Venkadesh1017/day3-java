1.find the count number;
import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner a = new Scanner(System.in);
        int num=a.nextInt();
        int count=0;
       while(num>0)
       {
           num=num/10;
           count++;
           
        }
         System.out.print(num);
      
    }
}
